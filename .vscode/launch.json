{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch ioBroker Adapter ",
			"skipFiles": [
				"<node_internals>/**"
			],
			"args": [
				"--debug",
				"0"
			],
			"program": "${workspaceFolder}/src/main.ts",
			"cwd": "${workspaceFolder}/.dev-server/default",
			//"runtimeExecutable": "c:/Users/oweit/AppData/Roaming/fnm/aliases/default/node.exe",
			"stopOnEntry": true,
			"sourceMaps": true,
			"resolveSourceMapLocations": [
				"${workspaceFolder}/**",
				"!**/node_modules/**"
			],
			"outFiles": [
				"${workspaceFolder}/.dev-server/default/node_modules/iobroker.pajdr/build/**/*.js"
			]
		}
	]
}
